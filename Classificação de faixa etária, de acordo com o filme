numero1 = parseFloat(prompt("Diga um numero: "))
numero2 = parseFloat(prompt("Diga um  segundo numero: "))
numero3 = parseFloat(prompt("Diga um  terceiro numero: "))

maiorNumero = numero1

if (isNaN(numero1) || isNaN(numero2) || isNaN(numero3)){
    console.log("Escreva somente nÃºmeros")
} else {
    if (numero2 > maiorNumero){
    maiorNumero = numero2
    } else if (numero3 > maiorNumero){
    maiorNumero = numero3
    }

console.log(maiorNumero)
}

